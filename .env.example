# Copy this file to .env and fill in your values.
# Used by scripts/generate-prebuilt.js

ANTHROPIC_API_KEY=sk-ant-...
ELEVENLABS_API_KEY=...

# Your ElevenLabs voice ID (find it in the ElevenLabs dashboard → Voices)
ELEVENLABS_VOICE_ID=...

# TMDB API key — free at https://www.themoviedb.org/settings/api
# Used at runtime to fetch movie poster images for the story cards.
EXPO_PUBLIC_TMDB_API_KEY=...

# Anthropic API key — used at runtime for on-demand story generation.
# Add this to Netlify env vars so users can generate stories without their own key.
EXPO_PUBLIC_ANTHROPIC_API_KEY=sk-ant-...
